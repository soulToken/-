.register-item{
  border-top: 5px solid #eee;
  border-bottom: 5px solid #eee;
  position: relative;
  padding: 16px;
}
.register-hosp-bar{
  position: relative;
}
.hosp-icon{
  width: 1.5rem;
  height: 1.5rem;
  display: inline-block;
  margin-right: 5px;
}
.hosp-icon image{
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
.hosp-name{
  font-size: 15px;
  color: #333;
}
.dept-name{
  color: #bbb;
  font-size: 14px;
  margin-left: 16px;
}
.register-key-value{
  padding: 5px;
  font-size: 14px;
}
.register-key{
  color: #333;
  display: inline-block;
  width: 5em;
}
.register-value{
  color: #333;
}
scroll-view{
  height: 100%;
}
.cancel-register-btn{
  font-size: 14px;
  color: #bbb;
  text-align: right;
}
.swiper-tab-content{
  position:absolute;
  left:0;
  top:0;
  bottom:0;
  width:100%;
  padding-top:88rpx
}
page{
  height:100%;
  width:100%;
  position: relative;
}
swiper{
  height:100%;
  /* box-sizing: border-box; */
}
.group{
  position: relative;
}
.status{
  position:absolute;
  right:0;
  top:50%;
  margin-top: -65px;
}
.cancelAlert{
  /* width:168rpx;
  height:50rpx; */
  background:rgba(255,128,0,1);
border-radius:28px;
color:rgba(255,255,255,1);
padding:5px 10px;
}
