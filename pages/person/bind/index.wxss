.notice_top {
  font-size: 14px;
  font-family: PingFangSC-Regular;
  color: rgba(165, 165, 165, 1);
  line-height: 20px;
  text-align: center;

  width:500rpx;
  text-align:center;
  margin:39px auto 25px auto;
 
}

.person-container {
  background: rgba(247, 247, 247, 1);
  overflow: hidden;
}

page {
  height: 100%;
