  flex-direction: column;
}

.register-status-icon {
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
}

.register-status-icon image {
  width: 100%;
  height: 100%;
}

.register-status-text {
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 80rpx;
  display: inline-block;
}

.register-text {
  color: #000000;
