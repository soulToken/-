page,
.container{
  background: #F7F7F7;
}
.weui-plan{
  background: white;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

.weui-plan-title{
  padding: 16px 16px;
  position: relative;
  font-size: 36rpx;
  color: #000;
}
.weui-plan-title::before{
  content: " ";
  position: absolute;
  top: 16px;
  bottom: 16px;
  left: 5px;
  border-left: 3px solid #0F98E3;
}
.weui-plan-title::after{
  content:" ";
  left: 0;
  right: 0;
  bottom: 0;
  height: 1px;
  border-bottom: 1px solid #D9D9D9;
  position: absolute;
  transform-origin: 0 0;
  transform: scaleY(0.5);
}
.weui-plan-value{
  float: right;
  font-size: 14px;
  color: #bbb;
  position: relative;
}
.is-link .weui-plan-value{
  padding-right:10px; 
}
.is-link .weui-plan-value::after{
  content: " ";
  width: 7px;
  height: 7px;
  border-top:2px solid #bbb;
  border-right: 2px solid #bbb;
  position: absolute;
  top: 50%;
  right: 0px;
  transform: translateY(-50%) rotate(45deg); 
}
.weui-plan.is-link:active{
  background: #EFEFEF;
}
.register-plan-content{
  background: white;
  overflow: hidden;
}
.date-bar{
  padding-top: 16px;
  display: flex;
}
.date-item{
  flex: 1;
  text-align: center;
  float: left;
  height: 90rpx;
  line-height: 1.2;
  margin-right: 10px;
  padding: 2px 16px;
  border: 1px solid #E5E5E5;
  box-shadow: 3px;
  border-radius: 30rpx;
}

.date-item-week{
  font-size: 24rpx;
  color: #737373;
  white-space:nowrap; 
}
.date-item-date{
  font-size: 36rpx;
  text-align: center;
  color: #737373;
}
.date-item.selected{
  background: #B18354;
  color: white;
}
.date-item.selected .date-item-date{
  color: white;
}
.date-item.selected .date-item-week{
  color: white;
}

.time-box{
  padding-top: 16px;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}
.time-item{
  width: 25%;
  font-size: 36rpx;
  margin-bottom:10px;
  float: left; 
  box-sizing: border-box;
}
.time-text{
  margin-right: 10px;
  border: 1px solid #E5E5E5;
  box-shadow: 3px;
  color: #737373;
  text-align: center;
  padding: 5px 16px;
  border-radius: 30rpx;
  box-sizing: border-box;
}
.time-text.selected{
  color: white;
  background: #B18354;
}

.btn-box{
  padding: 16px;
}

.register-btn{
  background-color: #B18354;
  color: white;
  border-radius: 15px;
  width:100%;
}
.register-btn:active{
  background-color: #b19272;
}

.register-plan{
  background: white;
  padding: 16px;
}
.register-plan-title{
  color: #000;
  font-size: 32rpx;
}
.register-time-content{
  margin-top: 16px;
}





.register-dlg-content{
  background: white;
  padding: 80rpx 5px 50rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.register-status-icon{
  width: 180rpx;
  height: 180rpx;
  margin: 0 auto;
}
.register-status-icon image{
  width: 100%;
  height: 100%;
}

.register-status-text{
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 80rpx;
  display: inline-block;
}
.register-text{
  color: #000000;
  font-size: 34rpx;
  display: block;
}
.reigster-date{
  color: #7F7F7F;
  font-size: 30rpx;
  display: block;
  margin-top: 10px;
}